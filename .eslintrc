{
  "extends": "airbnb",
  "rules": {
    "linebreak-style": 0,
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "max-len": ["error", { "code": 200 }],
    "import/no-extraneous-dependencies": "off",
    "import/prefer-default-export": "off"
  },
  "parser": "babel-eslint",
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"],
        "paths": ["./src"]
      }
    }
  }
}